{
    "name": "bouledepate/askwey",
    "description": "Answers and questions platform",
    "type": "project",
    "require": {
        "php": ">=8.1",
        "yiisoft/yii2": "^2.0",
        "yiisoft/yii2-bootstrap5": "*",
        "vlucas/phpdotenv": "^5.4",
        "kartik-v/yii2-widget-activeform": "dev-master",
        "kartik-v/yii2-widget-datetimepicker": "dev-master",
        "kartik-v/yii2-widget-timepicker": "dev-master",
        "kartik-v/yii2-widget-datepicker": "dev-master",
        "kartik-v/yii2-widget-alert": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Askwey\\App\\": "app/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    }
}
